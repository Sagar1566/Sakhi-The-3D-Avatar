# Client Environment Variables (Next.js)
# Copy this file to .env.local and fill in your actual values

# WebSocket URL
# Development: ws://localhost:8000/ws/test-client
# Production: wss://your-api-domain.com/ws/test-client
NEXT_PUBLIC_WS_URL=ws://localhost:8000/ws/test-client

# API URL
# Development: http://localhost:8000
# Production: https://your-api-domain.com
NEXT_PUBLIC_API_URL=http://localhost:8000

# Application Metadata
NEXT_PUBLIC_APP_NAME=Sakhi - The 3D Avatar
NEXT_PUBLIC_APP_VERSION=1.0.0

# Disable Next.js Telemetry (Optional)
NEXT_TELEMETRY_DISABLED=1
